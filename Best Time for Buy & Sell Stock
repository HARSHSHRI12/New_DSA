class Solution {
public:
    int maxProfit(vector<int>& prices) {
         if(prices.empty()) return 0;
        int maxprofite=0;
        int bestbuy=prices[0];
        for(int i=1;i<prices.size();i++){
            if(prices[i]>bestbuy){
                maxprofite=max(maxprofite,prices[i]-bestbuy);
            }
            bestbuy=min(bestbuy,prices[i]);
        }
   return maxprofite;
        
    }
};
